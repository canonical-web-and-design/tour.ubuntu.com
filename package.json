{
  "name": "ubuntu-online-tour",
  "description": "An online tour guiding the user through the main features of Ubuntu.",
  "scripts": {
    "serve": "http-server",
    "build": "gulp compress-html compress-images",
    "clean": "rm -rf node_modules yarn-error.log *.log *.sqlite",
    "watch": "node-sass --include-path node_modules --source-map true --watch static/sass --output static/css",
    "test": "sass-lint css/*.css --verbose --no-exit"
  },
  "author": "",
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-inline-source": "^2.1.0",
    "imagemin-pngquant": "^4.2.0",
    "node-sass": "^4.5.3",
    "sass-lint": "^1.10.2"
  },
  "dependencies": {
    "http-server": "^0.10.0"
  }
}
